<li>
      <% game_feed_item.teams.each_with_index do |team, index| %>
		<div class="team">
			<%= raw team.players.map { |player| link_to(player.display_name_for_result(current_player, game_feed_item), player_game_path(player, @game)) }.join(" <span class=\"ampersand\">&amp;</span> ") %>
        	<% if index == 0 %>
    	</div>
    	<div class="defeated-wrap">
			<span class="defeated">defeated</span>
		</div>
        	<% end %>
      <% end %>
</li>